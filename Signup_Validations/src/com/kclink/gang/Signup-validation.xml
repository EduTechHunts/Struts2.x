    <field-validator type="requiredstring">
       <message>Last Name is required.</message>
    </field-validator>
</field>
 
<field name="email">
    <field-validator type="requiredstring">
    <message>Enter email</message>
    </field-validator>
 
    <field-validator type="email">
    <message>enter valid email id</message>
    </field-validator>
</field>
